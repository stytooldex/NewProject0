<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="8dp"
    tools:context="com.example.blade.activity.search_mik.ShellFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView2_su"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:autoLink="all"
            android:textIsSelectable="true"
            android:textStyle="bold"
            tools:text="TextView" />
    </ScrollView>

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/val_fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="16dp"
        android:text="执行"
        android:textColor="@android:color/white"
        android:theme="@style/Widget.MaterialComponents.ExtendedFloatingActionButton.Icon"
        android:visibility="gone"
        app:elevation="0dp"
        app:icon="@drawable/ic_near_me_black_24dp_fot"
        tools:ignore="HardcodedText" />
</FrameLayout>
